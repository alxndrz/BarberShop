<table class="table table-striped">
  <thead class="thead-dark">
    <tr>
      <th>ID</th>
      <th>Имя</th>
      <th>Телефон</th>
      <th>Дата</th>
      <th>Барбер</th>
      <th>Цвет</th>
    </tr>
  </thead>

  <tbody>
    <% @results.each do |row| %>
      <tr>
        <td><%= row['id'] %></td>
        <td><%= row['name'] %></td>
        <td><%= row['phone'] %></td>
        <td><%= row['datestamp'] %></td>
        <td><%= row['master'] %></td>
        <td style="background-color: <%= row['color']%>"><% row['color'] %></td>
      </tr>
    <% end %>
  </tbody>



</table>